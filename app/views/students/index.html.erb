<div>
  <% @students.each do |student| %>
    <div><%= student.first_name + " " + student.last_name %></div>
  <% end %>
</div>


<div>
  <%@students.each do |student|%>
  <div><a href='<%="/students/#{student.id}"%>'><%= student.first_name + " " + student.last_name %></a></div>
  <% end %>
</div>


<div>
<%@students.each do |student|%>
<div><%= link_to student.to_s, "/students/#{student.id}"%></div>
<%end%>
</div>

<div>
<%@students.each do |student|%>
<div><%= link_to student.to_s, student_path(student.id)%></div>
<%end%>
</div>


<div>
<%@students.each do |student|%>
<div><%= link_to student.to_s, student_path(student)%></div>
<%end%>
</div>
